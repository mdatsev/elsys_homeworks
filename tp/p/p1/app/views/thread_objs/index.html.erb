<p id="notice"><%= notice %></p>

<h1>Thread Objs</h1>

<table>
  <thead>
    <tr>
      <th>Thread</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @thread_objs.each do |thread_obj| %>
      <tr>
        <td><%= thread_obj.thread_id %></td>
        <td><%= thread_obj.value %></td>
        <td><%= link_to 'Show', thread_obj %></td>
        <td><%= link_to 'Edit', edit_thread_obj_path(thread_obj) %></td>
        <td><%= link_to 'Destroy', thread_obj, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Thread Obj', new_thread_obj_path %>
